option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: mysite/mysite/wsgi.py
    NumProcesses: 3
    NumThreads: 20
  "aws:elasticbeanstalk:container:python:staticfiles":
    "/static/": "www/static/"
   

  aws:elasticbeanstalk:application:environment:
    PATH: /usr/local/gdal/bin:$PATH
    LD_LIBRARY_PATH: /usr/local/proj4/lib:/usr/local/gdal/lib:$LD_LIBRARY_PATH
    GDAL_DATA: /usr/local/gdal/share/gdal

    DJANGO_SETTINGS_MODULE: "mysite.settings.dev"
    "PYTHONPATH": "/opt/python/current/app/mysite:$PYTHONPATH"
